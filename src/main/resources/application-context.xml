<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <!-- Define the Transaction bean -->
    <bean id="transaction" class="DWTour.Tour.entity.Transaction">
        <property name="id" value="1" />
        <property name="quantity" value="Transaction Description" />
        <property name="total" value="1000.0" />
        <property name="status_payment" value="1000.0" />
        <property name="trip" value="1000.0" />
    </bean>

</beans>